{
  "name": "nexrel-service-template",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx watch server/_core/index.ts",
    "build": "vite build && esbuild server/_core/prod.ts --platform=node --packages=external --bundle --format=esm --outfile=api/index.js && node scripts/copy-public.mjs && node scripts/vercel-output.mjs",
    "build:direct": "vite build && esbuild server/_core/prod.ts --platform=node --packages=external --bundle --format=esm --outfile=api/index.js && node scripts/copy-public.mjs",
    "start": "NODE_ENV=production node api/index.js",
    "check": "tsc --noEmit",
    "format": "prettier --write .",
    "db:migrate": "node scripts/db-migrate.mjs",
    "db:push": "drizzle-kit generate && drizzle-kit migrate",
    "db:seed": "node seed-db.mjs",
    "db:import": "node scripts/import-listings.mjs",
    "db:sync-centris": "node scripts/sync-centris-listings.mjs",
    "vercel-build": "npm run build",
    "kb:docx": "node scripts/convert-kb-to-docx.mjs"
  },
  "dependencies": {
    "@daily-co/daily-js": "^0.87.0",
    "@elevenlabs/client": "^0.11.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@tanstack/react-query": "^5.90.2",
    "@trpc/client": "^11.6.0",
    "@trpc/react-query": "^11.6.0",
    "@trpc/server": "^11.6.0",
    "apify-client": "^2.22.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "embla-carousel-react": "^8.6.0",
    "express": "^4.21.2",
    "i18next": "^25.8.11",
    "i18next-browser-languagedetector": "^8.2.1",
    "lucide-react": "^0.453.0",
    "nanoid": "^5.1.5",
    "pg": "^8.18.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hook-form": "^7.64.0",
    "react-i18next": "^16.5.4",
    "sonner": "^2.0.7",
    "superjson": "^1.13.3",
    "tailwind-merge": "^3.3.1",
    "wouter": "^3.3.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@mohtasham/md-to-docx": "^2.6.1",
    "@tailwindcss/vite": "^4.1.3",
    "@types/express": "4.17.21",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^24.7.0",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.1",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.31.4",
    "esbuild": "^0.25.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^4.1.14",
    "tsx": "^4.19.1",
    "typescript": "5.9.3",
    "vite": "^7.1.7"
  }
}
